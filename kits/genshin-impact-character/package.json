  "devDependencies": {
    "@randomkits/eslint-config": "workspace:*",
    "@randomkits/prettier-config": "workspace:*",
    "@randomkits/tsconfig": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "tsup": "catalog:"
  },
  "dependencies": {
    "@randomkits/utils": "workspace:*"
  },
  "prettier": "@randomkits/prettier-config"
}
